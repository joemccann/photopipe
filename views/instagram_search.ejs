<!DOCTYPE html>
<html>
<%- include head %>
<body data-twitter-auth="<%= isTwitterAuth %>" data-facebook-auth="<%= isFacebookAuth %>" data-instagram-auth="<%= isInstagramAuth %>" data-dropbox-auth="<%= isDropboxAuth %>" data-current-network='<%= currentNetwork %>'>
  <div id="main">
    
    <%- include nav %>

    <%- include search_form %>
    
    <section class="gallery right-search-gallery" id="photo-picker-instagram">
      
      <div class="one-up-wrapper" id="one-up-instagram-wrapper">

        <div id="one-up-instagram" class="one-up">

          <span class="close-one-up" data-icon="&#x2297;"></span>

          <div class="use-photo-container">
            <a class="button save use-photo" id="instagram-use-photo">This One!</a>
          </div>

        </div>

      </div> <!-- end .one-up-wrapper -->    
      
      <div class="spin-container">
        <div id="spin"></div>
      </div>
      
      <div class="load-more hide">
        <a class="button add" data-pagination="" id="instagram-load-more" href="/instagram/get_next_page_of_instagram_photos">LOAD MORE PHOTOS</a>
      </div>

    </section>

    <%- include debug %>
    
  </div> <!-- end #main -->
  
  <iframe src="" id="downloader" class="none"></iframe>
  
	<div id="overlay"></div>
  
  <%- include scripts %>
</body>
<!--

  Oh hi, there.  You're viewing source so you're probably somewhat
