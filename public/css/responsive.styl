/* 1em = 16px */
@media only screen and (min-width: 30em) and (max-width: 45em)
  
  body
    padding 1% 3%

  h1
    font-size 2.8em

  #images > img
    padding 2% 1% 2% 1%
    
@media only screen and (min-width: 20em) and (max-width: 29.9999em)

  body
    padding 1% 3%

  h1
    font-size 2.4em

  #images > img
    padding 2% 6% 2% 1%
    